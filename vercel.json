{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/src/server.js"
        }
    ],
    "env": {
        "VERIFY_TOKEN": "isJustString"
    }
}
